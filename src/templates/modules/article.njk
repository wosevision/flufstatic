<div class="row">
	<div class="offset-lg-2 col-lg-8 offset-md-1 col-md-10">
		<h2>{{ module.title }}</h2>
		<p>{{ module.text }}</p>
		{% if module.blockquoteText %}
		<blockquote>
			{{ module.blockquoteText }}
			<cite>- Client Name</cite>
		</blockquote>
		{% endif %}
		<p>{{ module.text }}</p>
	</div>
</div>
